{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "id": "moonlight",
  "name": "Moonlight",
  "author": "Nishant Varma",
  "themes": [
    {
      "name": "Moonlight",
      "appearance": "dark",
      "style": {
        "palette": {
          "note": "color semantics for consistency",
          "green": "#77dd77 — creation: functions, attributes, success, additions",
          "yellow": "#ffe066 — values: strings, text literals",
          "cyan": "#66e5e5 — types: type annotations, info",
          "pink": "#ff79c6 — control: keywords, operators, tags",
          "purple": "#bd93f9 — constants: booleans, numbers, special vars",
          "red": "#ff5555 — danger: errors, deletions",
          "orange": "#ffb86c — caution: warnings, escapes, regex",
          "gray": "#555555 — noise: comments, muted elements",
          "white": "#ffffff — neutral: variables, cursor, active elements",
          "base": "#b3b3b3 — default: text, punctuation, icons"
        },

        "background": "#1a1a1a",
        "background.appearance": "opaque",
        "elevated_surface.background": "#252525",
        "surface.background": "#171717",
        "panel.background": "#171717",
        "panel.focused_border": "#14dbc450",

        "title_bar.background": "#171717",
        "title_bar.inactive_background": "#171717",
        "status_bar.background": "#171717",
        "toolbar.background": "#1a1a1a",

        "tab_bar.background": "#171717",
        "tab.active_background": "#1a1a1a",
        "tab.inactive_background": "#171717",

        "border": "#141414",
        "border.disabled": "#00000050",
        "border.focused": "#14dbc450",
        "border.selected": "#14dbc450",
        "border.transparent": "#00000000",
        "border.variant": "#ffffff10",

        "text": "#b3b3b3",
        "text.accent": "#77dd77",
        "text.disabled": "#444444",
        "text.muted": "#888888",
        "text.placeholder": "#b3b3b350",

        "icon": "#b3b3b3",
        "icon.accent": "#77dd77",
        "icon.disabled": "#444444",
        "icon.muted": "#888888",
        "icon.placeholder": "#b3b3b350",

        "editor.background": "#1a1a1a",
        "editor.foreground": "#b3b3b3",
        "editor.gutter.background": "#1a1a1a",
        "editor.line_number": "#5a5a5a",
        "editor.active_line.background": "#00000050",
        "editor.active_line_number": "#ffffff",

        "scrollbar.thumb.background": "#ffffff15",
        "scrollbar.thumb.hover_background": "#ffffff25",
        "scrollbar.thumb.border": "#00000000",
        "scrollbar.track.background": "#00000000",
        "scrollbar.track.border": "#00000000",

        "players": [
          {
            "cursor": "#ffffff",
            "selection": "#ffffff15",
            "background": "#ffffff33"
          }
        ],

        "terminal.background": "#151515",
        "terminal.foreground": "#b3b3b3",

        "created": "#77dd77",
        "modified": "#ffe066",
        "deleted": "#ff5555",
        "conflict": "#ff79c6",
        "hidden": "#555555",
        "ignored": "#555555",
        "renamed": "#66e5e5",

        "error": "#ff5555",
        "error.background": "#ff555533",
        "error.border": "#ff5555",

        "warning": "#ffb86c",
        "warning.background": "#ffb86c33",
        "warning.border": "#ffb86c",

        "info": "#66e5e5",
        "info.background": "#66e5e533",
        "info.border": "#66e5e5",

        "success": "#77dd77",
        "success.background": "#77dd7733",
        "success.border": "#77dd77",

        "hint": "#888888",
        "hint.background": "#88888815",
        "hint.border": "#888888",

        "predictive": "#555555",
        "predictive.background": "#55555515",
        "predictive.border": "#555555",

        "link_text.hover": "#66e5e5",
        "drop_target.background": "#ffffff15",
        "unreachable": "#555555",

        "syntax": {
          "attribute": { "color": "#77dd77", "font_style": "italic" },
          "boolean": { "color": "#bd93f9" },
          "comment": { "color": "#555555", "font_style": "italic" },
          "comment.doc": { "color": "#555555" },
          "constant": { "color": "#bd93f9" },
          "function": { "color": "#77dd77" },
          "keyword": { "color": "#ff79c6" },
          "number": { "color": "#bd93f9" },
          "operator": { "color": "#ff79c6" },
          "punctuation": { "color": "#b3b3b3" },
          "string": { "color": "#ffe066" },
          "string.escape": { "color": "#ffb86c" },
          "string.regex": { "color": "#ffb86c", "font_style": "italic" },
          "string.special": { "color": "#ffe066" },
          "tag": { "color": "#ff79c6" },
          "type": { "color": "#66e5e5", "font_style": "italic" },
          "variable": { "color": "#ffffff" },
          "variable.special": { "color": "#bd93f9", "font_style": "italic" }
        }
      }
    }
  ]
}
