{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "id": "moonlight",
  "name": "Moonlight",
  "author": "Nishant Varma",
  "themes": [
    {
      "name": "Moonlight",
      "appearance": "dark",
      "style": {
        "palette": {
          "green": "#c3e88d — strings, attributes",
          "blue": "#82aaff — functions",
          "yellow": "#ffcb6b — types",
          "purple": "#c792ea — keywords",
          "orange": "#f78c6c — constants, numbers",
          "cyan": "#89ddff — operators",
          "red": "#f07178 — tags, errors",
          "gray": "#676e95 — comments",
          "white": "#eeffff — variables",
          "base": "#a6accd — punctuation"
        },

        "background": "#121212",
        "background.appearance": "opaque",
        "elevated_surface.background": "#2c2c2c",
        "element.hover": "#333333",
        "element.selected": "#252525",
        "ghost_element.hover": "#333333",
        "ghost_element.selected": "#252525",
        "surface.background": "#121212",
        "panel.background": "#121212",
        "panel.focused_border": "#82aaff",

        "title_bar.background": "#121212",
        "title_bar.inactive_background": "#121212",
        "status_bar.background": "#121212",
        "toolbar.background": "#121212",

        "tab_bar.background": "#121212",
        "tab.active_background": "#222222",
        "tab.inactive_background": "#121212",

        "border": "#121212",
        "border.disabled": "#0a0a0a",
        "border.focused": "#82aaff",
        "border.selected": "#82aaff",
        "border.transparent": "#121212",
        "border.variant": "#1c1c1c",

        "text": "#eeffff",
        "text.accent": "#82aaff",
        "text.disabled": "#4a5568",
        "text.muted": "#a6accd",
        "text.placeholder": "#4a5568",

        "icon": "#eeffff",
        "icon.accent": "#82aaff",
        "icon.disabled": "#4a5568",
        "icon.muted": "#a6accd",
        "icon.placeholder": "#4a5568",

        "editor.background": "#121212",
        "editor.foreground": "#eeffff",
        "editor.gutter.background": "#121212",
        "editor.line_number": "#4a5568",
        "editor.active_line.background": "#1a1a1a",
        "editor.active_line_number": "#eeffff",

        "scrollbar.thumb.background": "#2a2a2a",
        "scrollbar.thumb.hover_background": "#333333",
        "scrollbar.thumb.border": "#121212",
        "scrollbar.track.background": "#121212",
        "scrollbar.track.border": "#121212",

        "players": [
          {
            "cursor": "#ffcb6b",
            "selection": "#2a2a2a",
            "background": "#333333"
          }
        ],

        "terminal.background": "#1a1a1a",
        "terminal.foreground": "#eeffff",

        "created": "#c3e88d",
        "modified": "#ffcb6b",
        "deleted": "#f07178",
        "conflict": "#c792ea",
        "hidden": "#676e95",
        "ignored": "#676e95",
        "renamed": "#89ddff",

        "error": "#f07178",
        "error.background": "#2d1f1f",
        "error.border": "#f07178",

        "warning": "#ffcb6b",
        "warning.background": "#2d2a1f",
        "warning.border": "#ffcb6b",

        "info": "#82aaff",
        "info.background": "#1f2230",
        "info.border": "#82aaff",

        "success": "#c3e88d",
        "success.background": "#1f2d1f",
        "success.border": "#c3e88d",

        "hint": "#676e95",
        "hint.background": "#1a1a1a",
        "hint.border": "#676e95",

        "predictive": "#676e95",
        "predictive.background": "#1a1a1a",
        "predictive.border": "#676e95",

        "link_text.hover": "#82aaff",
        "drop_target.background": "#2a2a2a",
        "unreachable": "#666666",

        "syntax": {
          "attribute": { "color": "#c3e88d", "font_style": "italic" },
          "boolean": { "color": "#f78c6c" },
          "comment": { "color": "#676e95", "font_style": "italic" },
          "comment.doc": { "color": "#676e95" },
          "constant": { "color": "#f78c6c" },
          "function": { "color": "#82aaff" },
          "keyword": { "color": "#c792ea" },
          "number": { "color": "#f78c6c" },
          "operator": { "color": "#89ddff" },
          "punctuation": { "color": "#a6accd" },
          "string": { "color": "#c3e88d" },
          "string.escape": { "color": "#f78c6c" },
          "string.regex": { "color": "#f78c6c", "font_style": "italic" },
          "string.special": { "color": "#c3e88d" },
          "tag": { "color": "#f07178" },
          "type": { "color": "#ffcb6b", "font_style": "italic" },
          "variable": { "color": "#eeffff" },
          "variable.special": { "color": "#f78c6c", "font_style": "italic" }
        }
      }
    }
  ]
}
