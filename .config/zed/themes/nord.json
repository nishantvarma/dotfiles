{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "id": "nord",
  "name": "Nord",
  "author": "Nishant Varma",
  "themes": [
    {
      "name": "Nord",
      "appearance": "dark",
      "style": {
        "palette": {
          "nord0": "#2e3440 — background",
          "nord1": "#3b4252 — elevated",
          "nord2": "#434c5e — selection",
          "nord3": "#4c566a — comments",
          "nord4": "#d8dee9 — text",
          "nord7": "#8fbcbb — types",
          "nord8": "#88c0d0 — info, decorators",
          "nord9": "#81a1c1 — keywords",
          "nord10": "#5e81ac — functions",
          "nord11": "#bf616a — errors",
          "nord12": "#d08770 — constants",
          "nord13": "#ebcb8b — strings, warnings",
          "nord14": "#a3be8c — success",
          "nord15": "#b48ead — numbers"
        },

        "background": "#2e3440",
        "background.appearance": "opaque",
        "elevated_surface.background": "#3b4252",
        "element.hover": "#434c5e",
        "element.selected": "#434c5e",
        "ghost_element.hover": "#434c5e",
        "ghost_element.selected": "#434c5e",
        "surface.background": "#2e3440",
        "panel.background": "#2e3440",
        "panel.focused_border": "#88c0d0",

        "title_bar.background": "#2e3440",
        "title_bar.inactive_background": "#2e3440",
        "status_bar.background": "#3b4252",
        "toolbar.background": "#2e3440",

        "tab_bar.background": "#2e3440",
        "tab.active_background": "#3b4252",
        "tab.inactive_background": "#2e3440",

        "border": "#3b4252",
        "border.disabled": "#2e3440",
        "border.focused": "#88c0d0",
        "border.selected": "#88c0d0",
        "border.transparent": "#2e3440",
        "border.variant": "#3b4252",

        "text": "#d8dee9",
        "text.accent": "#88c0d0",
        "text.disabled": "#4c566a",
        "text.muted": "#a5abb6",
        "text.placeholder": "#4c566a",

        "icon": "#d8dee9",
        "icon.accent": "#88c0d0",
        "icon.disabled": "#4c566a",
        "icon.muted": "#a5abb6",
        "icon.placeholder": "#4c566a",

        "editor.background": "#2e3440",
        "editor.foreground": "#d8dee9",
        "editor.gutter.background": "#2e3440",
        "editor.line_number": "#4c566a",
        "editor.active_line.background": "#3b4252",
        "editor.active_line_number": "#d8dee9",

        "scrollbar.thumb.background": "#434c5e",
        "scrollbar.thumb.hover_background": "#4c566a",
        "scrollbar.thumb.border": "#2e3440",
        "scrollbar.track.background": "#2e3440",
        "scrollbar.track.border": "#2e3440",

        "players": [
          {
            "cursor": "#d8dee9",
            "selection": "#434c5e",
            "background": "#88c0d0"
          }
        ],

        "terminal.background": "#2e3440",
        "terminal.foreground": "#d8dee9",

        "created": "#a3be8c",
        "modified": "#ebcb8b",
        "deleted": "#bf616a",
        "conflict": "#b48ead",
        "hidden": "#4c566a",
        "ignored": "#4c566a",
        "renamed": "#8fbcbb",

        "error": "#bf616a",
        "error.background": "#3b3640",
        "error.border": "#bf616a",

        "warning": "#ebcb8b",
        "warning.background": "#3b3b40",
        "warning.border": "#ebcb8b",

        "info": "#88c0d0",
        "info.background": "#303b42",
        "info.border": "#88c0d0",

        "success": "#a3be8c",
        "success.background": "#343b40",
        "success.border": "#a3be8c",

        "hint": "#4c566a",
        "hint.background": "#3b4252",
        "hint.border": "#4c566a",

        "predictive": "#4c566a",
        "predictive.background": "#3b4252",
        "predictive.border": "#4c566a",

        "link_text.hover": "#88c0d0",
        "drop_target.background": "#434c5e",
        "unreachable": "#4c566a",

        "syntax": {
          "attribute": { "color": "#8fbcbb", "font_style": "italic" },
          "boolean": { "color": "#81a1c1" },
          "comment": { "color": "#616e88", "font_style": "italic" },
          "comment.doc": { "color": "#616e88" },
          "constant": { "color": "#d08770" },
          "function": { "color": "#88c0d0" },
          "keyword": { "color": "#81a1c1" },
          "number": { "color": "#b48ead" },
          "operator": { "color": "#81a1c1" },
          "punctuation": { "color": "#d8dee9" },
          "string": { "color": "#a3be8c" },
          "string.escape": { "color": "#ebcb8b" },
          "string.regex": { "color": "#ebcb8b", "font_style": "italic" },
          "string.special": { "color": "#a3be8c" },
          "tag": { "color": "#81a1c1" },
          "type": { "color": "#8fbcbb", "font_style": "italic" },
          "variable": { "color": "#d8dee9" },
          "variable.special": { "color": "#d08770", "font_style": "italic" }
        }
      }
    }
  ]
}
